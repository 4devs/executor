{
  "name": "fdevs/executor",
  "type": "library",
  "description": "FDevs Executor Component",
  "keywords": [],
  "license": "MIT",
  "authors": [
    {
      "name": "Konstantin Vytyagov",
      "email": "vytyagov.k@gmail.com"
    },
    {
      "name": "4devs Community",
      "homepage": "https://github.com/4devs/executor/contributors"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.10",
    "symfony/dependency-injection": "^3.4 | ^4.0"
  },
  "suggest": {
    "symfony/dependency-injection": "Allow to use FDevs\\Executor\\DependentExecutableIterator implementation"
  },
  "config":{
    "bin-dir": "bin",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "FDevs\\Executor\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
